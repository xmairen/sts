body { font-family: 'Microsoft YaHei'; font-size: 12px; color: #444; margin: 0; padding: 0; border: 0 none; }
ul, li, h1, h2, h3, h4, h5, p, a, img,input,select,textarea,table{ padding: 0; margin: 0; border: 0 none;  outline: 0 none;}
h1 { font-weight: normal; font-size: 14px; }
a, a:hover, a *, a:hover * { text-decoration: none; cursor: pointer; border: 0 none; }
li { list-style-type: none; }
.lfloat { float: left; }
.rfloat { float: right; }
.clear { clear: both; }
i { font-style: normal; }
