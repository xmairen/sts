.container { position: absolute; width: 100%; _height: expression((document.documentElement.clientHeight||document.body.clientHeight)-50); top: 50px; bottom: 0; left: 0; right: 0; }
.container .menu { z-index: 8; position: absolute; left: 0; top: 0; width: 200px; background: #3a4050; }
.container .menu .menuLi { float: left; width: 100%; }

.container .hasChildren { position: relative; }

.container .menu .menuLi .menuLink { white-space: nowrap; float: left; width: 100%; display: block; height: 45px; position: relative; color: #90939c; font-size: 14px; border-bottom: 1px solid #2c303c; }
.container .menu .menuLi .menuLink span { display: inline-block; margin: 12px 0 0; float: left; }
